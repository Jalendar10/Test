	<xsl:template name="getTerminatedReasonDesc">
	
		<xsl:param name="code"/>
		
		<xsl:choose>
			<xsl:when test="$code = '01')">
				<xsl:value-of select="'Account Data Compromise'"/>
			</xsl:when>
			<xsl:when test="$code = '02')">
				<xsl:value-of select="'Common Points of Purchase'"/>
			</xsl:when>
			<xsl:when test="$code = '03')">
				<xsl:value-of select="'Laundering'"/>
			</xsl:when>
			<xsl:when test="$code = '04')">
				<xsl:value-of select="'Excessive Chargebacks'"/>
			</xsl:when>
			<xsl:when test="$code = '05')">
				<xsl:value-of select="'Excessive Fraud'"/>
			</xsl:when>
			<xsl:when test="$code = '06')">
				<xsl:value-of select="'Reserved for Future Use'"/>
			</xsl:when>
			<xsl:when test="$code = '07')">
				<xsl:value-of select="'Fraud Conviction'"/>
			</xsl:when>
			<xsl:when test="$code = '08')">
				<xsl:value-of select="'MasterCard Questionable Merchant Audit Program'"/>
			</xsl:when>
			<xsl:when test="$code = '09')">
				<xsl:value-of select="'Bankruptcy/Liquidation/Insolvency'"/>
			</xsl:when>
			<xsl:when test="$code = '10')">
				<xsl:value-of select="'Violation of MasterCard Standards'"/>
			</xsl:when>
			<xsl:when test="$code = '11')">
				<xsl:value-of select="'Merchant collusion'"/>
			</xsl:when>
			<xsl:when test="$code = '12')">
				<xsl:value-of select="'PCI Data Security Standard'"/>
			</xsl:when>
			<xsl:when test="$code = '13')">
				<xsl:value-of select="'Illegal Transactions'"/>
			</xsl:when>
			<xsl:when test="$code = '14')">
				<xsl:value-of select="'Identity Theft'"/>
			</xsl:when>
			
			<xsl:otherwise>
				<xsl:value-of select="''"/>
			</xsl:otherwise>
		</xsl:choose>
	
	</xsl:template>
	
