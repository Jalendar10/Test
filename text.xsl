function g2AdHocCall(){
	//var loggeduser = window.parent.jQuery(window.parent.document).contents().find("#user_menu_content div").find('span.headerValue').text().trim();	
	var loggeduser = RCMAPI.user.name;
	var httpObj = new XMLHttpRequest();	
	var res="plugin?plugin=G2AdhocCall&amp;module=G2AdhocExt&amp;page=main&amp;ownerID="+loggeduser;	
	var httpObj = new XMLHttpRequest();
	var url=encodeURI(res);
	url = url.replace(/%26/g,"%26amp%3B");
	url = url.replace(/%27/g,"%26apos%3B");
	url = url.replace(/%22/g,"%26quot%3B");		
	url = url.replace(/%3C/g,"%26lt%3B");
	url = url.replace(/%3E/g,"%26gt%3B");
	httpObj.open("POST", url, false);
	httpObj.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
	httpObj.onreadystatechange = function()
	{        	
		if(httpObj.status == 200)
			{					
				var res=httpObj.responseText;
				var parser = new DOMParser();
				var doc=parser.parseFromString(res,"text/html");						
				var scriptSource=jQuery(doc).find("#pluginDataLanguage")[0];								
				scriptSource.innerHTML=scriptSource.innerHTML.replace(/\\"/g, "'");
				scriptSource.innerHTML=scriptSource.innerHTML.replace("<pre>", "");
				scriptSource.innerHTML=scriptSource.innerHTML.replace("</pre>", "");						
				eval(scriptSource.innerHTML.replace(/,\n}/g, "}"));		
				if(merdata.G2Adhoc_Flag==true){
					$("#g2AdhocBtn").show();
				} else {
					$("#g2AdhocBtn").hide();
				}
			}
	};
	httpObj.send("");
}

