<script>
/*Two Way Connect Script Begins */	
var successfulEPMessage=null;
function sendTwoWayConnectSMS(){
	var PhoneNumber= document.getElementById("smsPhoneNumber").value;
	console.log("Phone Number Fetched is:"+PhoneNumber);
	if(PhoneNumber.trim()==''){
	alert("Please Enter the Phone Number");
	return false;
	}
	if(isNaN(PhoneNumber.trim())){
	alert("Please enter a Integer value in the Phone Number Field");
	return false;
	}
	var PortfolioID = '<xsl:value-of select="/enrichedAlert/content/Alert/Application/Portfolio"/>';
	var MerchantID= '<xsl:value-of select="/enrichedAlert/content/Alert/Application/Merchant/MID"/>';
	var ApplicationID='<xsl:value-of select="/enrichedAlert/content/Alert/ApplicationID"/>';
	document.getElementById("imgProcess").style.display = "inline";
	var httpObj = new XMLHttpRequest();
	var res="plugin?plugin=DiligenceTwoWayConnect&amp;module=DiligenceTwoWayConnectExtensionId&amp;page=DiligenceTwoWayConnectPageId&amp;ApplicationID="+ApplicationID+"&amp;MerchantID="+MerchantID+"&amp;PortfolioID="+PortfolioID+"&amp;PhoneNumber="+PhoneNumber;
	var url=encodeURI(res); 
	httpObj.open("POST", url,true);
	
	httpObj.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
	httpObj.onreadystatechange = function()
	
	{        	
		if(httpObj.status == 200 &amp;&amp; httpObj.readyState == 4)
			{	
				var res=httpObj.responseText;
				var parser = new DOMParser();
				var doc=parser.parseFromString(res,"text/html");						
				var scriptSource=jQuery(doc).find("#pluginSMSIdentifier")[0];
				scriptSource.innerHTML=scriptSource.innerHTML.replace(/\\"/g, "'");						
				eval(scriptSource.innerHTML.replace(/,\n}/g, "}"));
				successfulEPMessage=merdata.JsAPI_SMSIdentifier;
				console.log("EP Call Status"+successfulEPMessage);
				if(successfulEPMessage=='1'){
				smsRequestClick();
				}
				else{
				alert("SMS Request Process failed. Please try again");
				document.getElementById("imgProcess").style.display = "none";
				}
				}
	};
	httpObj.send("");
}
var smsCheck=null;
function smsRequestClick(){

  smsCheck = setInterval(function() {
  console.log("Function is called");
  if(successfulEPMessage=='1'){
  clearInterval(smsCheck);
	populateTwoWayConnectDetails();

	}
  }, 1000);
}

function populateTwoWayConnectDetails(){
	console.log("Two Way Connect Details Function is called");
	var MerchantID= '<xsl:value-of select="/enrichedAlert/content/Alert/Application/Merchant/MID"/>';
	var ApplicationID='<xsl:value-of select="/enrichedAlert/content/Alert/ApplicationID"/>';
	
	
	var httpObj = new XMLHttpRequest();
	var res="plugin?plugin=DiligenceTwoWayConnect&amp;module=DiligenceTwoWayConnectRefreshExtensionId&amp;page=DiligenceTwoWayConnectRefreshPageId&amp;ApplicationID="+ApplicationID+"&amp;MerchantID="+MerchantID;
	
	var url=encodeURI(res); 
	httpObj.open("POST", url, false);
	httpObj.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
	httpObj.onreadystatechange = function()
	
	{        	
		if(httpObj.status == 200 &amp;&amp; httpObj.readyState == 4)
			{	
			
				var res=httpObj.responseText;
				var parser = new DOMParser();
				var doc=parser.parseFromString(res,"text/html");						
				var scriptSource=jQuery(doc).find("#pluginMessageIdentifier")[0];
				scriptSource.innerHTML=scriptSource.innerHTML.replace(/\\"/g, "'");						
				eval(scriptSource.innerHTML.replace(/,\n}/g, "}"));
				console.log(merdata.JsAPI_ResponseIdentifier);
				populateSMSTable();
				console.log("Two Way Connect Logger after Populate SMS Table");
				console.log("Logger should be null");
				document.getElementById("imgProcess").style.display = "none";
				}
	};
	httpObj.send("");
}

function populateSMSTable(){
			console.log("SMS Table finction called");
			var table = document.getElementById("smsTable");
			var rowCount = table.rows.length;
				while(table.rows.length > 0) {
						table.deleteRow(0);
					}
			var response =new Array();
			var responseTimeStamp= new Array();
			
			for(var n=0; n &lt; merdata.JsAPI_ResponseIdentifier.length; n++)
				{ 
					responseTimeStamp.push(merdata.JsAPI_ResponseIdentifier[n].responseTimeStamp);
					response.push(merdata.JsAPI_ResponseIdentifier[n].response);
				}
			
			var tableRowsHtml = '';				
			for(var n=0; n &lt; merdata.JsAPI_RequestIdentifier.length; n++)
				{ 
					
					tableRowsHtml += '&lt;tr&gt;';	
													
					tableRowsHtml += '&lt;td style="text-align:center;"&gt;';
					tableRowsHtml+=merdata.JsAPI_RequestIdentifier[n].requestTimeStamp;
					
					tableRowsHtml +=  '&lt;/td&gt;';
					
					tableRowsHtml += '&lt;td style="text-align:center;"&gt;';
					tableRowsHtml+=merdata.JsAPI_RequestIdentifier[n].phoneNumber;
					
					tableRowsHtml +=  '&lt;/td&gt;';
					
					tableRowsHtml += '&lt;td style="text-align:center;"&gt;';
					tableRowsHtml+=responseTimeStamp[n];
					
					tableRowsHtml +=  '&lt;/td&gt;';
					
					tableRowsHtml += '&lt;td style="text-align:center;"&gt;';
					tableRowsHtml+=response[n];
					
					tableRowsHtml +=  '&lt;/td&gt;';
					tableRowsHtml += '&lt;/tr&gt;';	
				}
					$('#smsTable').append(tableRowsHtml);						

}	

/*Two Way Connect Script Begins */
</script>
