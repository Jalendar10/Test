function highlightChecklistQuestions(){	
	var elemID = "";
	var radioVal = "";
	var qNo = "";
	var tdID = "";
	for(var counter=1; counter &lt;=44; counter ++){
		elemID = "radiochk"+counter;	
		radioVal = "";				
		if (document.getElementById(elemID+"1").checked) {
			radioVal = document.getElementById(elemID+"1").value;
		} else if (document.getElementById(elemID+"2").checked) {
			radioVal = document.getElementById(elemID+"2").value;
		} else if (document.getElementById(elemID+"3").checked){
		radioVal = document.getElementById(elemID+"3").value;
		}
		qNo = "";
		if(counter%2 == 0)
			qNo = (counter/2);
		else{
			qNo = Math.round(parseInt(counter/2)) + 1;
		}				
		tdID = "q"+qNo;
		if(radioVal == ""){									
