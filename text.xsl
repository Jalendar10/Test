function fetchCADetailsOnLoad(){
	var AlertType='<xsl:value-of select="/enrichedAlert/attributes/attribute[@property='alerts.p13']/value"/>';
		var AlertID = '<xsl:value-of select="/enrichedAlert/@id"/>';
		var httpObj = new XMLHttpRequest();
		var res="plugin?plugin=DiligenceDecision_US&amp;module=DiligenceDecisionDetailsExt&amp;page=GetChecklistDetails&amp;AlertID="+AlertID;
		var url=encodeURI(res); 
		httpObj.open("POST", url, false);
		httpObj.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
		httpObj.onreadystatechange = function(){        	
			if(httpObj.status == 200 &amp;&amp; httpObj.readyState == 4){	
					var res=httpObj.responseText;
					//console.log(res);
					var parser = new DOMParser();
					var doc=parser.parseFromString(res,"text/html");						
					var scriptSource=jQuery(doc).find("#pluginDataChecklistValues")[0];
					scriptSource.innerHTML=scriptSource.innerHTML.replace(/\\"/g, "'");	
					
					eval(scriptSource.innerHTML.replace(/,\n}/g, "}"));
					
					var currentStatus='<xsl:value-of select="/enrichedAlert/status/@name"/>';
					
					if(currentStatus != 'Post Underwriting'){
						var caIndicator=merdata.JsAPI_ChecklistData.caIndicator;
						if(caIndicator !== 'undefined'){
							fetchCADesc(caIndicator);
						}							
						var rppDelayDays=merdata.JsAPI_ChecklistData.rppDelayDays;
						if(rppDelayDays !== 'undefined'){
							//alert(rppDelayDays);
							$("#rppDelayDaysCreditSummaryScreen").html(rppDelayDays);
						}
						/*var monthlyLimit = merdata.JsAPI_ChecklistData.fundingExclusionMonthlyLimit;
						var dailyLimit = merdata.JsAPI_ChecklistData.fundingExclusionDailyLimit;
						if(dailyLimit !== 'undefined' || dailyLimit !== '' ){
							$("#insertDecisionDailyLimit").html(dailyLimit);
						}
						if(monthlyLimit !== 'undefined' || monthlyLimit !== ''){
							$("#insertDecisionMonthlyLimit").html(monthlyLimit);
						}*/
					}	
				}	
		};
		
		httpObj.send("");
