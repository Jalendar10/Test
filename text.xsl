function getFormattedDate(dt){
		var split3=dt.split("-");
		var secondSplit=null;
      if (typeof split3 !== "undefined"){
      secondSplit=split3[2].split(" ");
      var initialsDate=new Date(secondSplit[0],split3[1]-1,split3[0]);
      }
      var monthNames = [
      "Jan", "Feb", "Mar",
      "Apr", "May", "Jun", "Jul",
      "Aug", "Sep", "Oct",
      "Nov", "Dec"
      ];
      var day = initialsDate.getDate();
      var monthIndex = initialsDate.getMonth();
      var year = initialsDate.getFullYear();
      var today = day+'-'+monthNames[monthIndex]+'-'+year.toString()+' '+secondSplit[1];
	  return today;
	}
	
