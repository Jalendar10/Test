function calculateDaysDifference(inputDate1,inputDate2){
   var date1 = new Date(inputDate1);
   var date2 = new Date(inputDate2); //date2 should be highest date
   var timeDiff = Math.abs(date2.getTime() - date1.getTime());
   var diffDays = (Math.abs(timeDiff / (1000 * 3600 * 24 * 365))).toFixed(2); 
   return diffDays;
  }  

