			}
	var postUnderwritingStep='<xsl:value-of select="/enrichedAlert/attributes/attribute[@property='alerts.p13']/value"/>';
	/* added some changes to existing code as part of Re-Decision on 09MAY2019 -- starts here --*/
	/*
	if(postUnderwritingStep == 'AUTO DECLINE'){
		document.getElementById('active').style.display='None';	
	}
	*/
	var CurrentStatus='<xsl:value-of select="/enrichedAlert/status/@name"/>';
	if(CurrentStatus == 'Approved' ){ 		/* || CurrentStatus == 'Declined' */
		if(postUnderwritingStep != 'AUTO APPROVE' &amp;&amp; postUnderwritingStep != 'AUTO DECLINE'){		
		document.getElementById('active').style.display='None';	
		}
	}
	AlertOwner = '<xsl:value-of select="/enrichedAlert/@ownerIdentifier"/>';
	//alert("postUnderwritingStep: "+postUnderwritingStep+" CurrentStatus: "+CurrentStatus+" AlertOwner:"+AlertOwner);
		if(AlertOwner === '' || (AlertOwner.toUpperCase() != LoggedInUser.toUpperCase())){
			if(!(postUnderwritingStep == 'AUTO APPROVE')){  /*  || postUnderwritingStep == 'AUTO DECLINE'  */
			document.getElementById('active').style.display='None';
			}
			if (CurrentStatus == 'In Process' || CurrentStatus =='Post Underwriting'){
			document.getElementById('Riskactive').style.display='None';
			//document.getElementById('prepaym').style.display='None';
			}
		}
		/*if((AlertOwner.toUpperCase() == LoggedInUser.toUpperCase()) &amp;&amp; (CurrentStatus == 'Declined' &amp;&amp; postUnderwritingStep == 'AUTO DECLINE')){
			//document.getElementById("reEvaluationActive").disabled=true;
		}*/
		/* validate logged in user roles -- starts here -- */
		/* &amp;&amp; postUnderwritingStep == 'AUTO DECLINE'*/
		if((AlertOwner.toUpperCase() == LoggedInUser.toUpperCase()) &amp;&amp; (CurrentStatus == 'Declined')){ 
			var n = 0; 
			var array = [];
			var queryId = 'Voltage_Fetch_Role_List_on_UserId';
			//var login_user_name = $($(window.parent.document).contents().find("#user_menu_content span")[0]).html();  //f5zriur
			var login_user_name = RCMAPI.user.name;
			//console.log(login_user_name);
			var ddqParam = [];
			ddqParam[0] = ['login_user_name',login_user_name]; 						
			RCMAPI.getDDQResult (queryId, function(result){		
			while(true){	
					if(result.getElementsByTagName("ddqItem")[n]){
						//console.log("Value obtaine is "+result.getElementsByTagName("ddqItem")[n].childNodes[0].nodeValue);
						let role = result.getElementsByTagName("ddqItem")[n].childNodes[0].nodeValue;
						if(role.indexOf('Diligence Credit Officer') != -1 || role.indexOf('Diligence Credit Manager') != -1){							
							array.push(role);	
						}	
						
					}
					else{
						break;
					}
					n++;
				}	
				if(array.length &lt; 1){
					document.getElementById('active').style.display='None';
				}
			}, ddqParam);
		}	
		
		/* validate logged in user roles -- ends here -- */
	}; 
	httpObj.send("");
	}
