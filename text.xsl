	for(var temp=1; temp &lt;= parseInt(totalPrincipals); temp++){
	var pullCBReport = '<xsl:value-of select = "/enrichedAlert/content/Alert/Application/Principals/Principal[1]/PullCBReport" />';
	var legalForm = '<xsl:value-of select = "/enrichedAlert/content/Alert/Application/Merchant/LegalForm" />';
		divValue = $("#radioSelectExpUS div#"+temp+" p").text();
		if(divValue.toUpperCase() === "Y"){
			if(isPullBureauCheck){
				isPullBureauCheck = false;
				if(legalForm.toUpperCase() === "GOVERNMENT" || legalForm.toUpperCase  === "PUBLIC"){
				  $("#radioSelectExpUS div#"+temp).html("<h3> No report </h3>");	
