		} 
	}
}

function populateWebsiteReview(){
	var count =1;
	var chkWebRev; 
	<xsl:if test="/enrichedAlert/content/Alert/WEBSITE_REVIEW">
		chkWebRev = "true";
		document.getElementById("newrec").value = "modify";
	</xsl:if>;
	//console.log("Tag present "+chkWebRev);
	if(chkWebRev=="true"){
		for(counter = 1;counter &lt;=22; counter ++){
			var str = '#q'+count;
			var obj = $(str);
			var qs = obj.html();
			var sel;		
			<xsl:for-each select="/enrichedAlert/content/Alert/WEBSITE_REVIEW/QUESTION">	
				sel = "<xsl:value-of select="Q"/>";
				var xmlVer = "<xsl:value-of select="VERIFIED"/>";
				var xmlCpv = "<xsl:value-of select="CPV"/>";
				//console.log(sel+" "+xmlVer+" "+xmlCpv);
				if(qs.trim() == sel.trim()){				
					var ver = obj.next('td').find("input:radio");
					for(i=0;i &lt;ver.length;i ++){
						if(ver[i].value == xmlVer.trim()){
							ver[i].checked = true;
						}
					}				
					var cpv = obj.next('td').next('td').find("input:radio");
					for(j = 0;j &lt;cpv.length;j++){
						if(cpv[j].value == xmlCpv.trim()){
							cpv[j].checked = true;
						}
					}
				}	
					//console.log("@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@");
				</xsl:for-each>;
			//console.log("------------------------------------------------");
			count++;	
		}
		var assessment = "<xsl:value-of select="/enrichedAlert/content/Alert/WEBSITE_REVIEW/ASSESSMENT"/>";		
		if(assessment.trim()=="pass"){
			$('#asses1').attr('checked', true);
			
		}
		else if(assessment.trim()=="fail"){
			$('#asses2').attr('checked', true);			
		}
		var dirName =  "<xsl:value-of select="/enrichedAlert/content/Alert/WEBSITE_REVIEW/DIRNAME"/>";
		document.getElementById("dirname").value = dirName;
		
		document.getElementById("signoffdate").value= "<xsl:value-of select="/enrichedAlert/content/Alert/WEBSITE_REVIEW/SIGNOFFDATE"/>";
		 
		var myEle = document.getElementById("webrev");
		if(myEle){
			myEle.value = "Y";
		}
	}
		var valMer = "<xsl:value-of select="/enrichedAlert/content/Alert/WEBSITE_REVIEW/VALIDMER"/>";
		if(valMer.trim()=="invalid"){
			$('#inPcValid2').attr('checked', true);
		}
		else{
			$('#inPcValid1').attr('checked', true);
		}	
		document.getElementById("currdate").innerHTML = "<xsl:value-of select="/enrichedAlert/content/Alert/WEBSITE_REVIEW/CREATEDDATE"/>";
		document.getElementById("moddate").innerHTML = "<xsl:value-of select="/enrichedAlert/content/Alert/WEBSITE_REVIEW/MODIFIEDDATE"/>";
