currentValuePrincReqMatch+=1;

var totalResponse= parseInt(<xsl:if test="/enrichedAlert/content/Alert/Application/Principals/Principal">
			<xsl:for-each select="/enrichedAlert/content/Alert/Application/Principals/Principal">
				<xsl:if test="position() = last()">
					'<xsl:value-of select= "position()"/>'
				</xsl:if>
			</xsl:for-each>
			</xsl:if>)+0;
var forward = 'matchPrincReqForward' + String(count);
var backward = 'matchPrincReqBackward' + String(count);

if(totalResponse === 0) {
document.getElementById(forward).disabled=true;
document.getElementById(backward).disabled=true;
}
else if(totalResponse === 1) {
document.getElementById(forward).disabled=true;
document.getElementById(backward).disabled=true;
divArrayPrincReqMatch[0].style.display='table-row-group';
}
else if(currentValuePrincReqMatch === totalResponse - 1) {
document.getElementById(forward).disabled=true;
divArrayPrincReqMatch[previousValue].style.display='none';
divArrayPrincReqMatch[currentValuePrincReqMatch].style.display='table-row-group';
}
else{
document.getElementById(forward).disabled=false;
divArrayPrincReqMatch[previousValue].style.display='none';
divArrayPrincReqMatch[currentValuePrincReqMatch].style.display='table-row-group';
}

//divArrayPrincReqMatch[previousValue].style.display='none';
//divArrayPrincReqMatch[currentValuePrincReqMatch].style.display='table-row-group';


document.getElementById(backward).disabled=false;
}


